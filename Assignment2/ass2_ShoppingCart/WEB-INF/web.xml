<web-app>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
	<servlet>
        <servlet-name>Part1</servlet-name>
        <servlet-class>MyCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part2</servlet-name>
        <servlet-class>ViewCart</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>Part1</servlet-name>
        <url-pattern>/mycart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Part2</servlet-name>
        <url-pattern>/viewcart</url-pattern>
    </servlet-mapping>
   
   

    
</web-app>
